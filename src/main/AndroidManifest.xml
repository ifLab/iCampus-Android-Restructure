<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.iflab.icampus" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity android:name=".OAuthActivity" >
        </activity>
        <activity
            android:name=".PersonalActivity"
            android:label="@string/title_activity_personal" >
        </activity>
        <activity
            android:name=".YelloPageActivity"
            android:label="@string/title_activity_yello_page_" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".YellowPageDetailActivity"
            android:label="@string/title_activity_contact_person" >
        </activity>

    </application>

</manifest>
